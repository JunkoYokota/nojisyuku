    <%= form_for current_user do |f| %>
    <h2>プロフィール画像</h2>
      <div class="form-group file">
        <%= f.file_field :image, class: "form-control floating-label", placeholder: "画像アップロード" %>
      </div>
      <h2>登録情報</h2>
      <div class="form-group">
        <div class="col-lg-6">
          <%= f.text_field :store_name, placeholder: "店名", class: "form-control" %>
        </div>
        <div class="col-lg-6">
          <%= f.text_field :loc, placeholder: "所在地", class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-lg-6">
          <%= f.text_field :phone, placeholder: "電話番号", class: "form-control" %>
        </div>
        <div class="col-lg-6">
          <%= f.text_field :hp_url, placeholder: "ホームページ", class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.submit "送信", class: "btn btn-primary withripple" %>
      </div>
    <% end %>